unit DataModuleProductos;

interface

uses
  SysUtils, Classes;

type
  TDMProducto = class(TDataModule)
  private
    { Private declarations }
  public
    { Public declarations }
  end;

function DMProducto: TDMProducto;

implementation

{$R *.dfm}

uses
  UniGUIVars, uniGUIMainModule, MainModule;

function DMProducto: TDMProducto;
begin
  Result := TDMProducto(UniMainModule.GetModuleInstance(TDMProducto));
end;

initialization
  RegisterModuleClass(TDMProducto);

end.
